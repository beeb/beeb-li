{
	"name": "beeb-li",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "biome lint src",
		"format": "biome format . --write && prettier --write src",
		"postbuild": "pagefind --site .vercel/output/static && rm -rf ./static/pagefind && cp -r ./.vercel/output/static/pagefind/ ./static/pagefind/"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.11",
		"@formatjs/intl-locale": "^5.2.1",
		"@iconify-json/lucide": "^1.2.93",
		"@iconify-json/mingcute": "^1.2.7",
		"@resvg/resvg-js": "^2.6.2",
		"@sveltejs/adapter-vercel": "^6.3.2",
		"@sveltejs/enhanced-img": "^0.10.3",
		"@sveltejs/kit": "^2.53.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.2.1",
		"asciinema-player": "^3.15.0",
		"daisyui": "^5.5.19",
		"hastscript": "^9.0.1",
		"mdsvex": "^0.12.6",
		"pagefind": "^1.4.0",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.5.0",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-external-links": "^3.0.0",
		"rehype-slug": "^6.0.0",
		"remark-abbr": "^1.4.2",
		"remark-toc": "^9.0.0",
		"satori": "^0.19.2",
		"shiki": "^3.22.0",
		"svelte": "^5.53.3",
		"svelte-check": "^4.4.3",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^4.2.1",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3",
		"unplugin-icons": "^23.0.1",
		"vite": "^7.3.1",
		"vite-imagetools": "^9.0.3",
		"vite-plugin-arraybuffer": "^0.1.4",
		"vite-plugin-data-url": "^1.0.0"
	},
	"type": "module",
	"packageManager": "pnpm@10.24.0",
	"pnpm": {
		"onlyBuiltDependencies": [
			"@biomejs/biome",
			"@tailwindcss/oxide",
			"esbuild",
			"sharp",
			"svelte-preprocess"
		]
	}
}
